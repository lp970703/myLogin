{
  "name": "mylogin",
  "version": "1.0.0",
  "description": "login",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.0.0",
    "egg-oauth2-server": "^2.2.6",
    "egg-scripts": "^2.17.0",
    "egg-sequelize": "^6.0.0",
    "egg-swagger-jsdoc": "0.0.3",
    "egg-view-ejs": "^3.0.0",
    "egg-view-nunjucks": "^2.3.0",
    "mysql2": "^3.2.0",
    "nodemailer": "^6.7.8",
    "sequelize": "^6.29.0",
    "swagger-jsdoc": "^6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "sequelize-cli": "^6.6.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-mylogin",
    "stop": "egg-scripts stop --title=egg-server-mylogin",
    "prod": "egg-scripts start --env=prod --workers=2",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "dev_db": "egg-sequelize-auto -o ./app/model -h 127.0.0.1 -p 3306 -d myproject -u root -x lupeng0703 -e mysql",
    "test_db": "egg-sequelize-auto -o ./app/model -h 127.0.0.1 -p 3306 -d dev_db -u root -x 111111 -e mysql",
    "pord_db": "egg-sequelize-auto -o ./app/model -h 127.0.0.1 -p 3306 -d dev_db -u root -x 111111 -e mysql",
    "dev_view": "npm i egg-bin cross-env easywebpack-cli easywebpack-vue egg-webpack egg-webpack-vue --save-dev",
    "clean": "easy clean all",
    "build": "easy build prod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "lopez",
  "license": "MIT"
}
